{
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "html-webpack-plugin": "^5.6.5",
    "husky": "^9.1.7",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "lint-staged": {
    "**/*.scss": [
      "npx --yes prettier --check --write"
    ],
    "**/*.{ts,tsx}": [
      "npx --yes eslint --fix",
      "npx --yes prettier --check --write"
    ],
    "src/lib.rs": [
      "rustfmt"
    ]
  },
  "name": "tetris-react-rs-wasm",
  "private": true,
  "scripts": {
    "build": "npm run build:rs && npm run build:react",
    "build:react": "webpack --mode=development",
    "build:rs": "npx --yes wasm-pack build --dev",
    "prepare": "husky",
    "release": "npm run release:rs && npm run release:react",
    "release:react": "webpack --mode=production",
    "release:rs": "npx --yes wasm-pack build --release",
    "start": "npm run build:rs && webpack serve --mode=development"
  }
}
